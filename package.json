{
  "name": "finance-tracker",
  "version": "0.0.1",
  "description": "A personal finance tracking application",
  "main": "app/electron-main.js",
  "scripts": {
    "build:renderer:prod": "cross-env NODE_ENV=production webpack --config webpack-renderer.config.ts --mode production",
    "build:electron-main:dev": "cross-env NODE_ENV=development webpack --config webpack-electron.config.ts --mode development",
    "build:electron-main:prod": "cross-env NODE_ENV=production webpack --config webpack-electron.config.ts --mode production",
    "build:app": "npm run build:renderer:prod && npm run build:electron-main:prod",
    "build:unpacked": "electron-builder --dir",
    "build:packed": "electron-builder",
    "build": "npm run build:app && npm run build:packed",
    "cleanup": "rimraf app && rimraf out",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "rebuild:better-sqlite3": "electron-rebuild -f -w better-sqlite3",
    "start:renderer": "webpack serve --config webpack-renderer.config.ts --mode development",
    "start:run-electron": "cross-env NODE_ENV=development electron ./app/electron-main.js",
    "start": "npm run build:electron-main:dev && concurrently \"cross-env npm run start:renderer\" \"npm run start:run-electron\" -n renderer,electron",
    "test:unit": "jest src/react",
    "test:e2e": "concurrently \"npm run start:renderer\" \"jest src/__tests__ --runInBand\""
  },
  "author": "Toni Ojala",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@reduxjs/toolkit": "^1.5.0",
    "better-sqlite3": "^7.1.2",
    "date-fns": "^2.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@hookform/devtools": "^2.2.1",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/better-sqlite3": "^5.4.1",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/dotenv-webpack": "^5.0.0",
    "@types/jest": "^26.0.19",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.14.19",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-env": "^1.16.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.2",
    "dotenv-webpack": "^6.0.0",
    "electron": "^10.1.1",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.3.4",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "react-test-renderer": "^17.0.1",
    "spectron": "^12.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  }
}
