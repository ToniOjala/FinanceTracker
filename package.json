{
  "name": "finance-tracker",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "build/electron/main.js",
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@reduxjs/toolkit": "^1.5.0",
    "better-sqlite3": "^7.1.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "compile": "tsc -p src/electron",
    "test": "concurrently \"npm start\" \"wait-on http://localhost:3000 && npm run compile && jest\"",
    "eject": "react-scripts eject",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && tsc -p src/electron -w\" \"wait-on http://localhost:3000 && tsc -p src/electron && electron .\"",
    "electron:build": "npm run build && npm run compile && electron-builder",
    "rebuild:better-sqlite3": "electron-rebuild -f -w better-sqlite3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "concurrently": "^5.3.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.1.0",
    "spectron": "^13.0.0",
    "ts-jest": "^26.4.4",
    "wait-on": "^5.2.1"
  }
}
